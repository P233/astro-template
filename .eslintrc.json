{
  "extends": ["plugin:astro/recommended", "plugin:astro/jsx-a11y-recommended"],
  "plugins": ["prettier"],
  "overrides": [
    {
      "files": ["*.astro"],
      "parser": "astro-eslint-parser",
      "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "extraFileExtensions": [".astro"]
      }
    }
  ],
  "rules": {
    "prettier/prettier": "warn"
  }
}

{
  "name": "astro-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "eslint": "./node_modules/.bin/eslint -c .eslintrc.js --fix ./src/**/*.astro",
    "stylelint": "./node_modules/.bin/stylelint --fix ./src/**/*.{astro,scss}",
    "prepare": "husky install"
  },
  "dependencies": {
    "astro": "^1.4.6"
  },
  "devDependencies": {
    "@p233/stylelint-config-scss": "^3.2.1",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "eslint": "^8.25.0",
    "eslint-plugin-astro": "^0.20.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "0.7.0",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0"
  },
  "lint-staged": {
    "*.astro": [
      "eslint --fix --max-warnings 0",
      "stylelint --fix"
    ],
    "!(package*).json": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}

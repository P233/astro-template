{
  "name": "astro-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "eslint": "./node_modules/.bin/eslint -c .eslintrc.js --fix ./src/**/*.astro",
    "stylelint": "./node_modules/.bin/stylelint --fix ./src/**/*.{astro,scss}",
    "prepare": "husky install"
  },
  "dependencies": {
    "astro": "^3.2.0",
    "astro-compress": "^2.0.15"
  },
  "devDependencies": {
    "@p233/stylelint-config-scss": "^4.0.0",
    "@typescript-eslint/parser": "^6.7.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.50.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3"
  },
  "lint-staged": {
    "*.astro": [
      "eslint --fix --max-warnings 0",
      "stylelint --fix"
    ],
    "!(package*).json": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
